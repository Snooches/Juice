    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\CommonClasses\CommonClasses.csproj" />
      <ProjectReference Include="..\PlayerBackend\PlayerBackend.csproj" />
    </ItemGroup>

</Project>
